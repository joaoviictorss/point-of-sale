{
  "$schema": "https://biomejs.dev/schemas/2.0.6/schema.json",
  "extends": [
    "ultracite"
  ],
  "linter": {
    "rules": {
      "nursery": {
        "noUnknownAtRule": "off",
        "useSortedClasses": "warn"
      },
      "performance": {
        "noNamespaceImport": "off"
      },
      "style": {
        "useNodejsImportProtocol": "off"
      }
    }
  }
}